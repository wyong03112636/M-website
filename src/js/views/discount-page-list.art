{{each list}}
<li>
    <div class="list-pic">
        <img src="https://ss0.bdstatic.com/70cFuHSh_Q1YnxGkpoWK1HF6hhy/it/u=1519312194,3777176648&fm=26&gp=0.jpg" data-src="{{$value.img}}" alt="" >
    </div>
    <div class="list-tit">
        <p>{{$value.title}}</p>
        <span>
            {{$value.subtitle}}
        </span>
        <div>
            <em>{{$value.business}}</em>
            <b>{{$value.data}}</b>
        </div>
    </div>
</li>
{{/each}}